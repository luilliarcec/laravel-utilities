{
    "name": "luilliarcec/laravel-utilities",
    "description": "Laravel Utilities it's a package that includes common utilities for projects.",
    "keywords": [
        "luilliarcec",
        "laravel-utilities",
        "laravel",
        "utilities"
    ],
    "homepage": "https://github.com/luilliarcec/laravel-utilities",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Luis Andr√©s Arce C.",
            "email": "luilliarcec@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "*",
        "spatie/laravel-package-tools": "^1.12"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "Luilliarcec\\Utilities\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "fix": "php-cs-fixer fix",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Luilliarcec\\Utilities\\UtilitiesServiceProvider"
            ]
        }
    }
}
